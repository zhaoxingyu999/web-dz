@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="zh-CN">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>WEB管理系统</title>
    <link href="@Html.GetUrlContent("/Content/css/login.css")" type="text/css" rel="stylesheet" />
</head>

<body>
    <div class="frontHome page" id="loginbox">
        <div class="wrap-container">
            <div id="home_container" class="clearfix">
                <div id="home_main">
                    <div class="inner-main">
                        <div class="login_box">
                            <div class="form-title">
                                <h3>WEB管理系统</h3>
                                <h4>Manager System</h4>
                            </div>
                            <div class="well form-horizontal" style="background:#FFF">
                                <div class="logininput">
                                    <input type='hidden' name='csrfmiddlewaretoken' value='29jf0poo0ghj7fgjArZNNHnEAdzg4Acs' />
                                    <fieldset>
                                        <div id="div_id_username" class="clearfix control-group">
                                            <label for="id_username" class="control-label requiredField">
                                                帐号<span class="asteriskField">*</span>
                                            </label>
                                            <div class="controls">
                                                <input class="textinput textInput" id="id_username" maxlength="30" name="username" placeholder="登录账号" type="text" />
                                            </div>
                                        </div>
                                        <div id="div_id_password" class="clearfix control-group">
                                            <label for="id_password" class="control-label requiredField">
                                                密码<span class="asteriskField">*</span>
                                            </label>
                                            <div class="controls">
                                                <input class="textinput textInput" id="id_password" name="password" placeholder="密码" type="password" />
                                            </div>
                                        </div>
                                        <div id="div_id_vercode" class="clearfix control-group">
                                            <label for="id_vercode" class="control-label requiredField">
                                                帐号<span class="asteriskField">*</span>
                                            </label>
                                            <div class="controls" style="width:120px; float:left;">
                                                <input class="textinput textInput" style="width:75%;" id="id_vercode" maxlength="5" name="vercode" placeholder="验证码" type="text" />
                                            </div>
                                            <div style="float:left;padding:20px 0 5px 15px; cursor:pointer;">
                                                <img id="vercodeImg" src="" style="height:42px;" />
                                            </div>
                                            <div style="float:left;padding:20px 0 10px 15px; line-height:41px;">
                                                <a href="javascript:void(0)" onclick="login.changeVerCode();">换一张</a>
                                            </div>
                                        </div>
                                    </fieldset>
                                </div>
                                <fieldset class="form-actions" style="position:relative; margin-top:0px;">
                                    <div class="loginFormBtn clearfix">
                                        <button class="login_btn js_login_btn" style="width:100%;">登    录</button>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="wrap-footer">
            <div id="footer">
                <div class="inner-footer">
                    <div class="foot">
                        Copyright © 2017 XXX.com XXX版权所有 备案XXX.com 渝ICP备XXX号-X
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="container" class="mpage">
        <div id="anitOut" class="anitOut"></div>
    </div>
</body>
</html>
<script src="@Html.GetUrlContent("/Scripts/jquery-2.2.0.js")" type="text/javascript"></script>
<script src="@Html.GetUrlContent("/Scripts/public/jquery.md5.js")" type="text/javascript"></script>
<script src="@Html.GetUrlContent("/Scripts/plugins/layer/layer.min.js")" type="text/javascript"></script>
<script src="@Html.GetUrlContent("/Scripts/public/myfuns.js")" type="text/javascript"></script>
<script src="@Html.GetUrlContent("/Scripts/WebManager/entrance/ani.js")" type="text/javascript"></script>
<script src="@Html.GetUrlContent("/Scripts/WebManager/entrance/login.js")" type="text/javascript"></script>