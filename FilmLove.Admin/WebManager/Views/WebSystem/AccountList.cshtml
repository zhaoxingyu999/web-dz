@{
    Layout = "~/Views/Shared/_Layout.cshtml";

    ViewBag.Title = "AccountList";
}
@Html.Partial("_nav", new FilmLove.Admin.CommEntity.ResEntities.NavigationModel("系统管理", "管理员列表"))
<div class="row bgwhitebox10 mymgb15">
    <div class="detail-search" style="border-bottom: 2px solid #01ACE2;padding-bottom:10px;">
        <div class="col-sm-9 mypdl0">
            关键字：
            <input type="text" id="KeyWord" class="mysearch-text" style="width:350px;" placeholder="账户名" />
        </div>
        <div class="col-sm-3 mypdr0" style="text-align:right;">
            <button id="searchBtn" class="btn btn-success btn-sm"><i class="fa fa-search"></i>查询</button>
            @Html.Partial("_linkBtn", "/WebSystem/AccountEdit")
        </div>
        <div class="mycl"></div>
    </div>
    <div class="row mtop15">
        <div class="row">
            <div class="col-sm-12" style="padding:0px;">
                <table class="table table-bordered mytable">
                    <thead>
                        <tr>
                            <th width="5%">序号</th>
                            <th width="10%">登录账户</th>
                            <th width="10%">真实姓名</th>
                            <th width="10%">联系电话</th>
                            <th width="10%">邮箱</th>
                            <th width="10%">创建时间</th>
                            <th width="10%">最后登录时间</th>
                            <th width="10%">人员状态</th>
                            <th width="12%">操作</th>
                        </tr>
                    </thead>
                    <tbody id="accountbody"></tbody>
                </table>
            </div>
            <div class="row">
                <div id="infoPage"></div>
            </div>
        </div>
    </div>
</div>
@section CssExtend{
    <link rel="stylesheet" href="@Html.GetUrlContent("/Scripts/plugins/sspager/ss_page.css")" />
}
@section JsExtend{
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/plugins/sspager/ss_page.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/WebManager/system/accountlist.js")"></script>
}

