@{
    Layout = null;

    var curAccount = WebManagers.Core.SysLoginInfo.CurAccount();
    string version = "";
    string file = Server.MapPath("/") + "version.txt";
    if (System.IO.File.Exists(file))
    {
        version = System.IO.File.ReadAllText(file);
        version = version.Replace("\r", "").Replace("\n", "");
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>WEB管理系统</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link rel="stylesheet" href="@Html.GetUrlContent("/Content/css/bootstrap.min.css")" />
    <link rel="stylesheet" href="@Html.GetUrlContent("/Content/css/font-awesome.min.css")">
    <link rel="stylesheet" href="@Html.GetUrlContent("/Content/css/ionicons.min.css")">
    <link rel="stylesheet" href="@Html.GetUrlContent("/Content/css/AdminLTE.min.css")">
    <link rel="stylesheet" href="@Html.GetUrlContent("/Content/css/skins/all-skins.min.css")">
    <link rel="stylesheet" href="@Html.GetUrlContent("/Content/css/myindex.css")">
    <!--[if lt IE 9]>
    <script src="@Html.GetUrlContent("/Scripts/plugins/ie9/html5shiv.min.js")"></script>
    <script src="@Html.GetUrlContent("/plugins/ie9/respond.min.js")"></script>
    <![endif]-->
    <link rel="stylesheet" href="@Html.GetUrlContent("/Content/css/mystyle.css")">
</head>
<body class="hold-transition skin-blue sidebar-mini fixed mybody">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <div class="logo">
                <span class="logo-mini"><b>YGL</b></span>
                <span class="logo-lg"><b>WEB管理系统</b></span>
            </div>
            <nav class="navbar navbar-static-top">
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">

                        <li class="messages-menu">
                            <a id="_editpassword" href="javascript:void(0);" class="dropdown-toggle">
                                <i class="fa fa-lock"></i> 修改密码
                            </a>
                        </li>
                        <li class="messages-menu">
                            <a href="/WebEntrance/LogOut" class="dropdown-toggle">
                                <i class="fa fa-sign-out"></i> 退出
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="main-sidebar">
            <section class="sidebar">
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="/Content/images/user2-160x160.jpg" class="img-circle" style="max-height:45px;" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>@curAccount.ManagerName</p>
                        <a href="javascript:void(0)"><i class="fa fa-circle text-success"></i> @curAccount.ManagerRealname</a>
                    </div>
                </div>
                <ul class="sidebar-menu"></ul>
            </section>
            <div style="position:fixed ; bottom:0px;width:100px;color: white;">version:@version</div>

        </aside>
        <div class="content-wrapper" id="content-wrapper" style="min-height: 421px;">
            <div class="content-tabs">
                <nav class="page-tabs menuTabs tab-ui-menu" id="tab-menu" style="margin-left:5px;">
                    <div class="page-tabs-content" style="margin-left: 0px;">
                    </div>
                </nav>
                <div class="btn-group roll-nav roll-right">
                    <button class="dropdown tabClose" data-toggle="dropdown">
                        页签操作<i class="fa fa-caret-down" style="padding-left: 3px;"></i>
                    </button>
                    <ul class="dropdown-menu dropdown-menu-right" style="min-width: 128px;">
                        <li><a class="tabReload" href="javascript:refreshTab();">刷新当前</a></li>
                        <li><a class="tabCloseCurrent" href="javascript:closeCurrentTab();">关闭当前</a></li>
                        <li><a class="tabCloseAll" href="javascript:closeOtherTabs(true);">全部关闭</a></li>
                        <li><a class="tabCloseOther" href="javascript:closeOtherTabs();">除此之外全部关闭</a></li>
                    </ul>
                </div>
                <button class="roll-nav roll-right fullscreen" onclick="App.handleFullScreen()">
                    <i class="fa fa-arrows-alt"></i>
                </button>
            </div>
            <div class="content-iframe " style="background-color: #ffffff; ">
                <div class="tab-content " id="tab-content">

                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/plugins/jQuery/jquery-2.2.3.min.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/public/jquery.md5.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/plugins/layer/layer.min.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/plugins/bootstrap/js/bootstrap.min.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/plugins/slimScroll/jquery.slimscroll.min.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/public/app.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/public/app_iframe.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/public/myfuns.js")"></script>
    <script type="text/javascript" src="@Html.GetUrlContent("/Scripts/WebManager/home/index.js")"></script>
</body>
</html>